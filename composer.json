{
    "name": "liulinnuha/simple-ai-client",
    "description": "Laravel AI SDK dengan multi provider AI (OpenAI, Gemini, DeepSeek)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Liulinnuha\\SimpleAiClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liulinnuha\\SimpleAiClient\\AiServiceProvider"
            ],
            "aliases": {
                "Ai": "Liulinnuha\\SimpleAiClient\\Facades\\Ai"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
